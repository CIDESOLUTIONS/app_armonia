model Report {
  id        Int      @id @default(autoincrement())
  reason    String
  listing   Listing  @relation(fields: [listingId], references: [id])
  listingId Int
  reporter  User     @relation(fields: [reporterId], references: [id])
  reporterId Int
  createdAt DateTime @default(now())

  @@index([listingId])
  @@index([reporterId])
}
